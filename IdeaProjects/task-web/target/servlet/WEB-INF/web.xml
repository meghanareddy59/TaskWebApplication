<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="WebApp_ID" version="2.5">

  <display-name>Servlet</display-name>


  <servlet>
    <description></description>
    <display-name>HelloWorld</display-name>
    <servlet-name>TaskServlet</servlet-name>
    <servlet-class>com.main.TaskServlet</servlet-class>
  </servlet>

  <servlet>
    <description></description>
    <display-name>IdServlet</display-name>
    <servlet-name>IdServlet</servlet-name>
    <servlet-class>com.main.IdServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TaskServlet</servlet-name>
    <url-pattern>/getTasks</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>IdServlet</servlet-name>
    <url-pattern>/Id</url-pattern>
  </servlet-mapping>


</web-app>
